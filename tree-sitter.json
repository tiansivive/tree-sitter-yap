{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "yap",
      "camelcase": "Yap",
      "title": "Yap",
      "scope": "source.yap",
      "file-types": [
        "yap"
      ],
      "highlights": "queries/highlights.scm",
      "injection-regex": "^yap$",
      "class-name": "TreeSitterYap"
    }
  ],
  "metadata": {
    "version": "0.1.1",
    "license": "MIT",
    "description": "A parser for the Yap language",
    "authors": [
      {
        "name": "Tiago Vila Verde"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-yap"
    }
  },
  "bindings": {
    "c": true,
    "go": false,
    "node": true,
    "python": false,
    "rust": false,
    "swift": false,
    "zig": false
  }
}
